{
    "name": "israeldominguez/laravel-logviewer",
    "description": "Log viewer for Genetsis Admin package",
    "type": "library",
    "version": "dev-master",
    "authors": [
        {
            "name": "idominguez",
            "email": "israel.dominguez.garcia@gmail.com"
        }
    ],
    "require": {
        "spatie/laravel-permission": "dev-master"
    },
    "require-dev": {
        "illuminate/support": "~5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Genetsis\\LogViewer\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genetsis\\LogViewer\\Providers\\LogViewerServiceProvider"
            ],
            "aliases": {
                "Logviewer": "Genetsis\\LogViewer\\Facades\\LogViewerFacade"
            }
        }
    }
}
